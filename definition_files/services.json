{
  "services": [
    {
      "name": "http_80",
      "protocol": "http",
      "listener": {
        "port": 80,
        "bind_ip": "0.0.0.0"
      },
      "headers": [
        "HTTP/1.1 200 OK",
        "Server: Honeyknot0.2",
        "Accept-Ranges: bytes",
        "Content-Type: text/html",
        "Connection: close"
      ],
      "responses": [
        {
          "match": "^HEAD *",
          "body": ""
        },
        {
          "match": "^GET *.php*",
          "body": "<?php system($_REQUEST['cmd']); ?>"
        },
        {
          "match": "^GET / *",
          "body": "<html><body><h2>success</h2></body></html>"
        },
        {
          "match": "^POST *",
          "body": "<html><body><h2>mysql_1.2.4 error</h2><br><h4>stacktrace</h4><br><p>1_0x0000013c#sysc(c_INSERT|c_SELECT|c_DELETE...)<br>2_0x0000013d#admin..p...2121admin!2121...<br>3_0x0000013e#segfault(sysc(callback_go))<br>...</p></body></html>"
        }
      ]
    },
    {
      "name": "ssh_222",
      "protocol": "tcp",
      "listener": {
        "port": 222,
        "bind_ip": "0.0.0.0"
      },
      "encoding": "utf-8",
      "responses": [
        {
          "match": "SSH-2.0-OpenSSH_*",
          "body": "SSH-2.0-OpenSSH_7.6p1 Ubuntu-4ubuntu0.3"
        }
      ]
    }
  ]
}
